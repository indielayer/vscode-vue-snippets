  "Vue Transition Component with JavaScript Hooks": {
    "prefix": "vtransition",
    "body": [
      "<Transition",
      "\t:css=\"false\"",
      "\t@before-enter=\"onBeforeEnter\"",
      "\t@enter=\"onEnter\"",
      "\t@after-enter=\"onAfterEnter\"",
      "\t@before-leave=\"onBeforeLeave\"",
      "\t@leave=\"onLeave\"",
      "\t@after-leave=\"onAfterLeave\"",
      ">",
      "\t${0}",
      "</Transition>"
    ],
    "description": "transition component js hooks"
  },
